{
    "name": "vedtam/myfrontend",
    "description": "Custom frontend scaffolding for my apps.",
    "type": "composer-plugin",
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/vedtam/myfrontend.git"
      }
    ],
    "require": {
      "vedtam/myfrontend":"dev-master"
    },
    "autoload": {
         "psr-4": {
             "Vedtam\\Presets\\MyFrontend\\": "src/"
         }
     },
     "extra": {
        "laravel": {
            "providers": [
                "Vedtam\\Presets\\MyFrontend\\MyFrontendServiceProvider"
            ]
        }
    }
}
